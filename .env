# DNAI Environment Variables
# Copy this file to .env and fill in your values

# Required: Gemini API Key (https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=isilah

# Required: JWT Secret Key (generate with: openssl rand -hex 32)
JWT_SECRET_KEY=your_super_secret_jwt_key_change_in_production

# Optional: JWT Token Expiry (default: 1440 minutes = 24 hours)
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Optional: Database Path (inside container)
DATABASE_PATH=/app/data/dnai.db
